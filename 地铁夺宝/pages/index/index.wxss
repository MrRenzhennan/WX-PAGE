.tx {
  font-size: 34rpx;
  color: #fff;
}

.start-work {
  width: 300rpx;
  height: 100rpx;
}

#object_bg {
  animation: object_one 5s linear 0s infinite normal;
}

@keyframes object_one {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}
