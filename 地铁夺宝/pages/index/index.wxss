.box {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  background: #5ed0eb;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bx {
  width: 100%;
  height: 769rpx;
  position: relative;
  /* display: flex;
  justify-content: center;
  align-items: center; */
}

.bx .bg {
  width: 600rpx;
  height: 600rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.bx .bg image{
  width: 100%;
  height: 100%;
}
.baoxiang{
  width: 500rpx;
  height: 466rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.baoxiang image{
  width: 100%;
  height: 100%;
}
.bizi{
