/* pages/editContent/editContent.wxss */
.line-blod {
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
}
.icon-right{
  width: 13rpx;
  height: 22rpx;
}
.list-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.list-box .list-item {
  padding: 30rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f5f5f5;
}
.item-label {
  font-size: 30rpx;
  color: #000;
  letter-spacing: 2rpx;
  font-family: PingFang-SC-Medium;
}