<!--pages/changeUserInfo/changeUserInfo.wxml-->
<!-- line -->
<view class="line-blod"></view>

<!-- list -->
<view class="list-box">
  <navigator url='/pages/applicationEligibility/applicationEligibility' hover-class="none">
    <view class="list-item">
      <text class="item-label">头像</text>
      <view class="item-right">
        <image class="user-head" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1575479555357&di=b4a7e7d771350bcc7e42b4fdf11115d7&imgtype=0&src=http%3A%2F%2Fku.90sjimg.com%2Felement_origin_min_pic%2F18%2F07%2F28%2F88723ef6ae8ef759f0f0ea5a0f882a1e.jpg%2521%2Ffwfh%2F804x804%2Fquality%2F90%2Funsharp%2Ftrue%2Fcompress%2Ftrue"></image>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </navigator>
  <view class="list-item">
    <text class="item-label">昵称</text>
    <view class="item-right">
      <input class="weui-input" placeholder-class="placeholderClass"  placeholder="请输入昵称" />
      <image class="icon-right" src="/static/imgs/icon_right.png"></image>
    </view>
  </view>
  <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
    <view class="list-item">
      <text class="item-label">性别</text>
      <view class="item-right">
        <text>{{array[index]}}</text>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </picker>
  <picker mode="date" value="{{date}}" start="1980-01-01" end="2020-01-01" bindchange="bindDateChange">
    <view class="list-item">
      <text class="item-label">生日</text>
      <view class="item-right">
        <text>{{date}}</text>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </picker>
  <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
    <view class="list-item">
      <text class="item-label">民族</text>
      <view class="item-right">
        <text>{{array[index]}}</text>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </picker>
  <view class="list-item">
    <text class="item-label">身份证号</text>
    <view class="item-right">
      <input class="weui-input" placeholder-class="placeholderClass"  placeholder="请输入身份证号" />
      <image class="icon-right" src="/static/imgs/icon_right.png"></image>
    </view>
  </view>
</view>
<view class="line-blod"></view>
<view class="list-box">
  <view class="list-item">
    <text class="item-label">手机号</text>
    <view class="item-right">
      <input class="weui-input" placeholder-class="placeholderClass"  placeholder="请输入手机号" />
      <image class="icon-right" src="/static/imgs/icon_right.png"></image>
    </view>
  </view>
  <view class="list-item">
    <text class="item-label">联系地址</text>
    <view class="item-right">
      <input class="weui-input" placeholder-class="placeholderClass"  placeholder="请输入联系地址" />
      <image class="icon-right" src="/static/imgs/icon_right.png"></image>
    </view>
  </view>
  <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
    <view class="list-item">
      <text class="item-label">学历背景</text>
      <view class="item-right">
        <text>{{array[index]}}</text>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </picker>
  <navigator url='' hover-class="none">
    <view class="list-item">
      <text class="item-label">我的二维码</text>
      <view class="item-right">
        <image class="wx-card" src="/static/imgs/icon_erweima.png"></image>
        <image class="icon-right" src="/static/imgs/icon_right.png"></image>
      </view>
    </view>
  </navigator>
</view>